{
  "name": "@nineteensevenfour/gate",
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "email": "bruno.duval@datatunning.com",
    "name": "Bruno DUVAL"
  },
  "keywords": [
    "micro-frontend",
    "angular",
    "netcore",
    "webapi",
    "typescript",
    "codegen",
    "nx",
    "nx-dotnet",
    "github actions"
  ],
  "bugs": {
    "url": "https://github.com/NineteenSevenFour/gate/issues"
  },
  "homepage": "https://github.com/NineteenSevenFour/gate/",
  "repository": {
    "type": "git",
    "url": "https://github.com/NineteenSevenFour/gate.git"
  },
  "private": true,
  "scripts": {
    "prepare": "git config core.ignorecase false && husky install",
    "prepare:gitflow": "git flow init --file ./gitflow.config",
    "lint": "nx run-many --target=lint",
    "lint:fix": "nx run-many --target=lint --fix",
    "format": "nx run-many --target=format",
    "format:fix": "nx run-many --target=format --fix",
    "pretest": "rimraf testresults",
    "test": "nx run-many --target=test",
    "checks": "npm run lint:fix && npm run format:fix && npm run test",
    "prebuild": "npm run pretest && rimraf -g dist **/obj",
    "build": "nx run-many --target=build --verbose",
    "prebuild:prod": "npm run pretest && rimraf -g dist **/obj",
    "build:prod": "nx run-many --target=build --configuration=production --verbose",
    "release": "nx run-many --target=semantic-release --parallel=false --verbose --ci=false --dryRun",
    "serve": "concurrently npm:serve:*",
    "serve:api": "nx run NineteenSevenFour.Gatehub:serve",
    "graph": "nx graph --watch",
    "sync-nuget-pkg": "npx nx g @nx-dotnet/core:sync",
    "reset": "npm cache verify && dotnet nuget locals all --clear && npx nx reset",
    "setup": "npm ci && dotnet restore && dotnet tool restore",
    "postsetup": "dotnet ef database update -c SqliteDbContext -s apps/gatehub -p libs/gatehub-data-sqlite --connection \"Data Source=C:\\storage\\data\\gate\\db\\gatehub.db\"",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "premigration:bundle": "rimraf C:\\storage\\data\\gate\\db\\*.* && npm run build",
    "migration:bundle": "concurrently npm:migration:bundle:*",
    "migration:bundle:sql": "dotnet ef migrations script --no-build -c SqliteDbContext -s apps/gatehub -p libs/gatehub-data-sqlite -o C:\\storage\\data\\gate\\db\\gatehub.bundle.sql",
    "migration:bundle:win64": "dotnet ef migrations bundle --no-build -r win-x64 -c SqliteDbContext -s apps/gatehub -p libs/gatehub-data-sqlite -o C:\\storage\\data\\gate\\db\\gatehub.bundle.win64.exe",
    "migration:bundle:lnx64": "dotnet ef migrations bundle --no-build -r linux-x64 -c SqliteDbContext -s apps/gatehub -p libs/gatehub-data-sqlite -o C:\\storage\\data\\gate\\db\\gatehub.bundle.lnx64.exe"
  },
  "resolutions": {
    "rxjs": "7.8.0"
  },
  "dependencies": {
    "@angular/animations": "17.1.3",
    "@angular/common": "17.1.3",
    "@angular/compiler": "17.1.3",
    "@angular/core": "17.1.3",
    "@angular/forms": "17.1.3",
    "@angular/platform-browser": "17.1.3",
    "@angular/platform-browser-dynamic": "17.1.3",
    "@angular/router": "17.1.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.2",
    "zone.js": "0.14.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.1.4",
    "@angular-devkit/core": "17.1.4",
    "@angular-devkit/schematics": "17.1.4",
    "@angular-eslint/eslint-plugin": "17.0.1",
    "@angular-eslint/eslint-plugin-template": "17.0.1",
    "@angular-eslint/template-parser": "17.0.1",
    "@angular/cli": "~17.1.0",
    "@angular/compiler-cli": "17.1.3",
    "@angular/language-service": "17.1.3",
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.4.4",
    "@nrwl/angular": "17.3.2",
    "@nrwl/eslint-plugin-nx": "17.3.2",
    "@nrwl/jest": "17.3.2",
    "@nrwl/js": "17.3.2",
    "@nrwl/linter": "17.3.2",
    "@nrwl/workspace": "17.3.2",
    "@nx-dotnet/core": "^1.21.1",
    "@schematics/angular": "17.1.4",
    "@theunderscorer/nx-semantic-release": "^2.5.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.16.9",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "all-contributors-cli": "^6.26.1",
    "concurrently": "^8.0.1",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "husky": "^8.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.6.2",
    "jest-preset-angular": "13.1.6",
    "ng-packagr": "17.1.2",
    "nx": "17.3.2",
    "openapi-typescript-codegen": "^0.25.0",
    "postcss": "^8.4.31",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.5.0",
    "postcss-url": "~10.1.3",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.3.3"
  }
}
