{
  "name": "@nineteensevenfour/gate",
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "email": "bruno.duval@datatunning.com",
    "name": "Bruno DUVAL"
  },
  "keywords": [
    "micro-frontend",
    "angular",
    "netcore",
    "webapi",
    "typescript",
    "codegen",
    "nx",
    "nx-dotnet",
    "github actions"
  ],
  "bugs": {
    "url": "https://github.com/NineteenSevenFour/gate/issues"
  },
  "homepage": "https://github.com/NineteenSevenFour/gate/",
  "repository": {
    "type": "git",
    "url": "https://github.com/NineteenSevenFour/gate.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "public"
  },
  "private": true,
  "scripts": {
    "prepare": "git config core.ignorecase false && husky install",
    "lint": "nx run-many --target=lint --exclude NineteenSevenFour.Gatehub, NineteenSevenFour.Gatehub.Test",
    "lint:fix": "nx run-many --target=lint --fix --exclude NineteenSevenFour.Gatehub, NineteenSevenFour.Gatehub.Test",
    "format": "nx run-many --target=format",
    "format:fix": "nx run-many --target=format --fix",
    "pretest": "rimraf testresults",
    "test": "nx run-many --target=test",
    "checks": "npm run lint:fix && npm run format:fix && npm run test",
    "prebuild": "npm run pretest && rimraf -g dist **/obj",
    "build": "nx run-many --target=build",
    "serve": "concurrently npm:serve:*",
    "serve:api":"nx run NineteenSevenFour.Gatehub:serve",
    "graph": "nx graph --wtch",
    "sync-nuget-pkg": "npx nx g @nx-dotnet/core:sync",
    "reset": "npm cache verify && dotnet nuget locals all --clear && npx nx reset",
    "setup": "npm ci && dotnet restore && dotnet tool restore",
    "postsetup": "concurrently npm:migration:*",
    "migration:clean": "rimraf gatehub.db",
    "migration:run": "dotnet ef database update -c SqliteDbContext -s apps/gatehub -p libs/gatehub-data-sqlite --connection \"Data Source=gatehub.db\""
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@nrwl/eslint-plugin-nx": "15.8.7",
    "@nrwl/jest": "15.8.7",
    "@nrwl/js": "^15.8.7",
    "@nrwl/linter": "15.8.7",
    "@nrwl/workspace": "15.8.7",
    "@nx-dotnet/core": "^1.21.1",
    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "concurrently": "^8.0.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "^8.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "nx": "15.8.7",
    "openapi-typescript-codegen": "^0.24.0",
    "prettier": "^2.6.2",
    "ts-jest": "^29.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.9.5"
  }
}
